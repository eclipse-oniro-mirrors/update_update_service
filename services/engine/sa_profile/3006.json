{
  "process": "updater_sa",
  "systemability": [
    {
      "name": 3006,
      "libpath": "libupdateservice.z.so",
      "run-on-create": true,
      "distributed": false,
      "bootphase": "BootStartPhase",
      "dump-level": 1,
      "auto-restart" : true,
      "start-on-demand": {
        "allow-update": true,
        "bindercall": true,
        "commonevent": [
          {
            "name": "usual.event.CONNECTIVITY_CHANGE",
            "value": "3"
          }
        ],
        "timedevent": [
          {
            "name": "loopevent",
            "value": "86400"
          }
        ]
      }
    }
  ]
}